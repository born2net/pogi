    "moment": "^2.15.1",
    "pg": "^6.0.3",
    "pg-query-stream": "^1.0.0"
  },
  "devDependencies": {
    "@types/jasmine": "^2.2.31",
    "@types/node": "^6.0.35",
    "babel-cli": "~6.14.0",
    "babel-plugin-transform-es2015-destructuring": "^6.16.0",
    "babel-plugin-transform-es2015-parameters": "^6.17.0",
    "babel-preset-es2015": "~6.14.0",
    "jasmine": "~2.4.1",
    "typescript": "^2.0.0"
  },
  "author": "Laszlo Radics<garaboncias@gmail.com>, Geza Radics<radicsge@protonmail.com>",
  "license": "MIT",
