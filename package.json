{
  "name": "pgdb-ts",
  "version": "0.9.0",
  "description": "An easy db handler over pg.js (node-postgres)",
  "main": "lib/pgdb.js",
  "typescript": {
    "definition": "pgdb.d.ts"
  },
  "scripts": {
    "build": "tsc -p src/tsconfig.json && babel lib/*.js -d ./",
    "test": "node --harmony spec/run.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/holdfenytolvaj/pgdb-ts.git"
